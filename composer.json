{
  "name": "jpeters/architect",
  "description": "Architect",
  "type": "library",
  "version": "0.8.0",
  "license": "MIT",
  "require": {
    "laravel/framework": "^8.0",
    "ext-json": "*",
    "laravel/ui": "^3.0",
    "doctrine/dbal": "^2.10|^3.0",
    "chartisan/php": "^1.2"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16",
    "orchestra/testbench": "^6.0",
    "phpmd/phpmd": "^2.8",
    "phpunit/phpunit": "^8.5|^9.0",
    "squizlabs/php_codesniffer": "^3.5",
    "jpeters/architect-test-helpers": "^0.4",
    "laravel/legacy-factories": "^1.0"
  },
  "authors": [
    {
      "name": "jamie peters",
      "email": "jamie@jamie-peters.co.uk"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "JPeters\\Architect\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JPeters\\Architect\\Tests\\": "Tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "JPeters\\Architect\\Providers\\ArchitectServiceProvider"
      ]
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/jpeters8889/architect-test-helpers"
    }
  ]
}
