{
  "name": "jpeters/architect",
  "description": "Architect",
  "type": "library",
  "version": "0.1.0",
  "license": "MIT",
  "require": {
    "laravel/framework": "^6.0",
    "ext-json": "*"
  },
  "authors": [
    {
      "name": "jamie peters",
      "email": "jamie@jamie-peters.co.uk"
    }
  ],
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "JPeters\\Architect\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JPeters\\Architect\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "JPeters\\Architect\\Providers\\ArchitectServiceProvider"
      ]
    }
  },
  "require-dev": {
    "nunomaduro/phpinsights": "dev-master",
    "friendsofphp/php-cs-fixer": "^2.17@dev",
    "phpmd/phpmd": "dev-master",
    "squizlabs/php_codesniffer": "^3.0@dev",
    "phpunit/phpunit": "8.5.x-dev",
    "orchestra/testbench": "4.4.1"
  }
}
