{
  "private": true,
  "scripts": {
    "architect-dev": "npm run architect-development",
    "architect-development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "architect-watch": "npm run architect-development -- --watch",
    "architect-watch-poll": "npm run architect-watch -- --watch-poll",
    "architect-hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "architect-prod": "npm run architect-production",
    "architect-production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
